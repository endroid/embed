{
    "name": "endroid/embed",
    "description": "Endroid Embed",
    "keywords": ["endroid", "embed"],
    "homepage": "https://github.com/endroid/embed",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "endroid/installer": "^1.1.5",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "endroid/test": "^1.1.2"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Embed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Embed\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
